<%= link_to t('shared.all.sem_back'), semesters_path %>
<div class="semester show">
  <h2><%= t('semesters.all.sem_details') %></h2>
  <table summary="Semester detail view">
    <tr>
      <th><%= t('semesters.all.sem_id') %></th>
      <td><%= @semester.sem_ID %></td>
    </tr>
    <tr>
      <th><%= t('students.all.std_name') %></th>
      <td><%= @semester.sem_name %></td>
    </tr>
    <tr>
      <th><%= t('semesters.all.sem_start_day') %></th>
      <td><%= @semester.sem_start %></td>
    </tr>
    <tr>
      <th><%= t('shared.all.createdat') %></th>
      <td><%= @semester.created_at %></td>
    </tr>
    <tr>
      <th><%= t('shared.all.updatedat') %></th>
      <td><%= @semester.updated_at %></td>
    </tr>
  </table>
</div>
